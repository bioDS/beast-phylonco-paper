<beast version="2.6" namespace="beast.core:beast.core.util:beast.evolution.datatype:beast.evolution.alignment:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood:phylonco.beast.evolution.substitutionmodel:phylonco.beast.evolution.errormodel:phylonco.beast.evolution.likelihood">
  <!-- Simulated diploid sequence alignment               -->
  <data id="alignment" dataType="binary">
    <sequence taxon="1">
            
        </sequence>
    <sequence taxon="2">
            
        </sequence>
    <sequence taxon="3">
            
        </sequence>
    <sequence taxon="4">
            
